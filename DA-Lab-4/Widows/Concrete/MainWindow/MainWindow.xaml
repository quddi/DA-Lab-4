<Window x:Class="DA_Lab_4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DA_Lab_4"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="537" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="LoadDependentDatasetButton" Content="Завантажити залежну вибірку" HorizontalAlignment="Center" Margin="0,0,0,135" VerticalAlignment="Bottom" Height="125" Width="512" FontSize="16" Click="LoadDependentDatasetButtonClick"/>
        <Button x:Name="LoadIndependentDatasetButton" Content="Завантажити незалежну вибірку" HorizontalAlignment="Center" Margin="0,0,0,10" Width="512" FontSize="16" Click="LoadIndependentDatasetButtonClick" Height="120" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
